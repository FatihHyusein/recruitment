{
  "name": "excitel_web",
  "version": "0.0.0",
  "description": "Excitel web app - created with angular 2",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=5.10"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "buildDev": "cross-env NODE_ENV=development webpack --display-chunks --progress --watch",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "del-cli ./dest",
    "lint": "npm-run-all lint:js lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js ./src ./server",
    "lint:ts": "tslint './src/**/*.ts'",
    "postinstall": "npm run typings",
    "prebuild": "npm-run-all clean",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' ./server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:dev",
    "typings": "typings install",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/forms": "0.1.1",
    "@angular/http": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/router": "3.0.0-alpha.8",
    "angular2-materialize": "^3.0.3",
    "core-js": "~2.4.0",
    "font-awesome": "^4.6.3",
    "hammerjs": "^2.0.8",
    "jquery": "^2.2.4",
    "materialize-css": "^0.97.7",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12"
  },
  "devDependencies": {
    "autoprefixer": "~6.3.6",
    "conventional-changelog-cli": "~1.2.0",
    "copy-webpack-plugin": "~3.0.1",
    "cross-env": "~1.0.8",
    "css-loader": "~0.23.1",
    "del-cli": "~0.2.0",
    "eslint": "~2.13.1",
    "express": "~4.14.0",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "~2.21.0",
    "imports-loader": "^0.6.5",
    "node-sass": "~3.8.0",
    "nodemon": "~1.9.2",
    "npm-run-all": "~2.3.0",
    "postcss-loader": "~0.9.1",
    "raw-loader": "~0.5.1",
    "sass-loader": "~4.0.0",
    "style-loader": "~0.13.1",
    "ts-loader": "~0.8.2",
    "tslint": "~3.12.1",
    "typescript": "~1.8.10",
    "typings": "~1.3.1",
    "webpack": "~1.13.1",
    "webpack-dev-server": "~1.14.1",
    "webpack-md5-hash": "~0.0.5",
    "winston": "~2.2.0"
  }
}
